{
  "name": "danidoble/routing",
  "description": "A easy way to start with symfony-routing",
  "type": "plugin",
  "homepage": "https://github.com/danidoble/routing",
  "license": "MIT",
  "suggest": {
    "spatie/ignition": "Render errors in a beautilful page",
    "symfony/error-handler": "Render errors in page"
  },
  "keywords": [
    "danidoble",
    "routing",
    "route",
    "php routing"
  ],
  "authors": [
    {
      "name": "danidoble",
      "email": "dani.doble.d@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "illuminate/support": "^v9.24",
    "ramsey/uuid": "^4.4",
    "symfony/routing": "^v6.1"
  },
  "require-dev": {
    "spatie/ignition": "^1.3",
    "vlucas/phpdotenv": "^v5.4"
  },
  "autoload": {
    "files": [
      "Src/global/global.php"
    ],
    "psr-4": {
      "Danidoble\\Routing\\": "Src"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "v1.0.0"
}
